{
  "name": "@template-node-tsup/lib",
  "type": "module",
  "version": "1.0.0",
  "private": false,
  "description": "",
  "author": "",
  "license": "MIT",
  "homepage": "",
  "repository": "",
  "bugs": "",
  "keywords": [],
  "packageManager": "^pnpm@7.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "init": "pnpm i",
    "lint:fix": "eslint . --fix",
    "dev": "cross-env RUNTIME_ENV=development esno watch play/index.ts",
    "build:split": "pnpm run clean && cross-env RUNTIME_ENV=production pnpm turbo run build:split",
    "build:all": "pnpm run clean && cross-env RUNTIME_ENV=production pnpm turbo run build:all",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "publish": "pnpm publish --no-git-checks --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "pnpm --filter @template-node-tsup/build run clean"
  },
  "peerDependencies": {
    "chalk": "4.1.2",
    "fs-extra": "^10.0.1",
    "ora": "^6.1.2"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "fs-extra": "^10.0.1",
    "ora": "^6.1.2"
  },
  "devDependencies": {
    "@baiwusanyu/eslint-config": "^1.0.6",
    "@changesets/cli": "^2.23.1",
    "@template-node-tsup/build": "*",
    "@template-node-tsup/entry": "*",
    "@template-node-tsup/runtime": "*",
    "@template-node-tsup/utils": "*",
    "@template-node-tsup/play": "*",
    "@types/chalk": "^2.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.0",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "esno": "^0.16.3",
    "pnpm": "^6.3.0",
    "tsup": "^6.2.3",
    "turbo": "^1.4.6",
    "typescript": "4.7.4"
  },
  "eslintConfig": {
    "extends": [
      "@baiwusanyu"
    ]
  }
}
